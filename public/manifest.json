{
  "manifest_version": 3,
  "name": "Healthy Contribution Bar",
  "version": "1.0.1",
  "action": {
    "default_icon": {
      "32": "icon32.png"
    },
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }
  ],
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://github.com/*",
    "https://github.com/*"
  ]
}